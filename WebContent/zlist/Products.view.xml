<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="zlist.Products" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Title">
		<content>
	        	<List
				id="ShortProductList"
				headerText="Products"
				items="{/d/results}">
		<items>
			<StandardListItem
				title="{ProductID}" info="{Name}" description="{Category}"/>
		</items>
	</List>
		</content>
	</Page>
</core:View>